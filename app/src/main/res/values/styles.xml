<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.Dialog.NoActionBar">
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="CustomActionBarTheme"
        parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>

    <!-- ActionBar styles -->
    <style name="MyActionBar"
        parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/blue_500</item>
    </style>

    <style name="MySettingsTheme" parent="android:Theme.Holo.Light">
        <item name="android:homeAsUpIndicator">@drawable/ic_arrow_back_white_24dp_padding</item>
        <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>

    <style name="MenuItemRippleStyle" parent="Material.Drawable.Ripple.Wave">
        <item name="rd_rippleColor">@color/md_grey_500</item>
        <item name="rd_rippleAnimDuration">100</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">0dp</item>
    </style>

    <!-- FloatingActionButton Style -->
    <style name="FloatingActionButton" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="rd_delayClick">none</item>
    </style>

    <style name="FloatingWaveActionButton" parent="Material.Drawable.Ripple.Wave.Light">
        <item name="fab_backgroundColor">#FFDFDFDF</item>
        <item name="fab_iconLineMorphing">@style/FloatingActionButtonIcon.Light</item>
        <item name="rd_delayClick">none</item>
    </style>

    <style name="FloatingColorActionButton" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="rd_rippleColor">#33000000</item>
        <item name="fab_backgroundColor">#FF2196F3</item>
        <item name="fab_iconLineMorphing">@style/FloatingActionButtonIcon</item>
        <item name="rd_delayClick">untilRelease</item>
    </style>

    <style name="FloatingWaveColorActionButton" parent="Material.Drawable.Ripple.Wave.Light">
        <item name="rd_rippleColor">#33000000</item>
        <item name="fab_backgroundColor">#FF2196F3</item>
        <item name="fab_iconLineMorphing">@style/FloatingActionButtonIcon</item>
        <item name="rd_delayClick">afterRelease</item>
    </style>

    <style name="FloatingActionButtonIcon" >
        <item name="lmd_state">@xml/fab_icon_states</item>
        <item name="lmd_curState">0</item>
        <item name="lmd_padding">2dp</item>
        <item name="lmd_animDuration">400</item>
        <item name="lmd_interpolator">@android:anim/accelerate_decelerate_interpolator</item>
        <item name="lmd_strokeSize">3dp</item>
        <item name="lmd_strokeColor">#FFFFFFFF</item>
        <item name="lmd_strokeCap">butt</item>
        <item name="lmd_strokeJoin">miter</item>
        <item name="lmd_clockwise">true</item>
    </style>

    <style name="FloatingActionButtonIcon.Light" parent="FloatingActionButtonIcon">
        <item name="lmd_strokeColor">#FF000000</item>
    </style>

    <!-- calendar colors -->
    <color name="mdtp_accent_color">@color/md_grey_600</color>
    <color name="mdtp_accent_color_dark">@color/md_grey_800</color>

